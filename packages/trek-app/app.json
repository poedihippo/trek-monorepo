{
  "expo": {
    "name": "TREK",
    "slug": "trek-app",
    "privacy": "hidden",
    "orientation": "portrait",
    "version": "1.0.0",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splashscreen.png",
      "resizeMode": "cover"
    },
    "userInterfaceStyle": "automatic",
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#313132",
      "translucent": false
    },
    "platforms": [
      "ios",
      "android"
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.trek.alba",
      "buildNumber": "1.0"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.trek.alba",
      "useNextNotificationsApi": true,
      "enableBackgroundNotification": true,
      "versionCode": 10,
      "permissions": [
        "CAMERA",
        "READ_PHONE_STATE",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "NOTIFICATIONS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/f115914d-e198-45c4-bc73-c538cf2c15d2"
    },
    "extra": {
      "eas": {
        "projectId": "f115914d-e198-45c4-bc73-c538cf2c15d2"
      }
    },
    "owner": "albatrek"
  }
}
